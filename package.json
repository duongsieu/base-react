{
  "name": "svhtt-tbspqc-fe",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-app-rewired start",
    "start": "cross-env NODE_ENV=development npm-run-all -p watch-css start-js",
    "build": "npm run build-css && cross-env NODE_ENV=production react-app-rewired build",
    "lint": "eslint --ext .jsx --ext .js .",
    "pretest": "npm run lint",
    "test": "set CI=true react-app-rewired test --env=jsdom --coverage --passWithNoTests",
    "test-watch": "react-app-rewired test --env=jsdom --coverage --passWithNoTests",
    "eject": "react-app-rewired eject",
    "enzyme": "react-app-rewired test --env=node"
  },
  "dependencies": {
    "@babel/core": "7.7.4",
    "@loadable/component": "5.10.1",
    "antd": "3.26.14",
    "async": "3.1.0",
    "axios": "0.19.1",
    "classnames": "2.2.6",
    "connected-react-router": "6.6.1",
    "file-saver": "2.0.2",
    "history": "4.10.1",
    "html-react-parser": "0.10.3",
    "humps": "2.0.1",
    "immutability-helper": "3.0.1",
    "less": "3.9.0",
    "less-loader": "5.0.0",
    "lodash": "4.17.15",
    "mobile-detect": "1.4.4",
    "moment": "2.24.0",
    "query-string": "6.10.1",
    "react": "16.8.6",
    "react-app-polyfill": "1.0.6",
    "react-app-rewired": "2.1.3",
    "react-cookie": "4.0.3",
    "react-custom-scrollbars": "4.2.1",
    "react-dnd": "9.5.1",
    "react-dnd-html5-backend": "9.5.1",
    "react-dnd-touch-backend": "9.5.1",
    "react-dom": "16.8.6",
    "react-google-recaptcha": "2.0.1",
    "react-quill": "1.3.5",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.0.1",
    "react-slick": "0.25.2",
    "recharts": "1.8.5",
    "redux": "4.0.5",
    "redux-axios-middleware": "4.0.1",
    "redux-thunk": "2.3.0",
    "slick-carousel": "1.8.1"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "babel-plugin-import": "1.13.0",
    "babel-plugin-lodash": "3.3.4",
    "cross-env": "5.2.0",
    "customize-cra": "0.2.13",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.4.4",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-babel": "9.0.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.13.0",
    "husky": "2.7.0",
    "jest-enzyme": "^7.0.2",
    "node-sass-chokidar": "1.3.5",
    "npm-run-all": "4.1.5",
    "react-test-renderer": "16.12.0",
    "react-testing-library": "8.0.1"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "text-summary"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ]
  }
}
